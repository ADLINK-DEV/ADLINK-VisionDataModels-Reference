[
    {
        "name":"DetectionPoint",
        "context":"com.adlinktech.vision.inference",
        "qosProfile":"telemetry",
        "version":"2.2.0",
        "description":"Inference engine results for object detection model outputting points. The stream_id is not part of this TagGroup and assumed to be part of the run-time flowId.",
        "tags":[
            {
                "name":"engine_id",
                "description":"Inference engine identifier",
                "kind":"STRING",
                "optional":true,
                "unit":"UUID"
            },
            {
                "name":"frame_id",
                "description":"ID of the input video frame fed to the inference engine",
                "kind":"INT64",
                "unit":"NUM"
            },
            {
                "name":"model_id",
                "description": "List of AI model identifiers associated with the result",
                "kind":"STRING_SEQ",
                "optional":true,
                "unit":"n/a"
            },
            {
                "name":"detection_point_data",
                "description":"List of Detection Point Data (the results)",
                "kind":"MULTI_DIM_NVP",
                "dimensions":1,
                "optional":true,
                "unit":"n/a",
                "typedefinition": "DetectionPointData"
            },
            {
                "name": "graph_data",
                "description": "List of Graphs (the objects)",
                "kind": "MULTI_DIM_NVP",
                "dimensions": 1,
                "optional": true,
                "unit": "n/a",
                "typedefinition": "Graph"
            }
        ]
    },
    {
        "typedefinition": "DetectionPointData",
        "tags": [
            {
                "name":"tracker_obj_id",
                "description":"Detected object id",
                "kind":"STRING",
                "optional":true,
                "unit":"UUID"
            },
            {
                "name":"super_category_id",
                "description":"Super category/class ID",
                "kind":"STRING",
                "optional":true,
                "unit":"n/a"
            },
            {
                "name":"super_category_label",
                "description":"Human interpreted super category name",
                "kind":"STRING",
                "optional":true,
                "unit":"n/a"
            },
            {
                "name":"category_id",
                "description":"List of classification category/class IDs",
                "kind":"STRING_SEQ",
                "optional":true,
                "unit":"n/a"
            },
            {
                "name":"category_label",
                "description":"List of human interpreted category names",
                "kind":"STRING_SEQ",
                "optional":true,
                "unit":"n/a"
            },
            {
                "name":"x",
                "description":"Center X Coordinate (% from 0,0)",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"y",
                "description":"Center Y Coordinate (% from 0,0)",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"radius",
                "description":"Size of the point (% of image width)",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"probability",
                "description":"Network confidence",
                "kind":"FLOAT64",
                "optional":true,
                "unit":"Percentage"
            },
            {
                "name":"color",
                "description":"Detection point color",
                "kind":"STRING",
                "optional":true,
                "unit":"Hex Code #RRGGBB"
            },
            {
                "name":"metadata",
                "description":"Buffer for extra inference metadata",
                "kind":"STRING",
                "optional":true,
                "unit":"N/A"
            }
        ]
    },
    {
        "typedefinition": "Graph",
        "tags": [
            {
                "name": "category_id",
                "description": "List of graph category/class IDs",
                "kind": "STRING_SEQ",
                "optional": true,
                "unit": "n/a"
            },
            {
                "name": "category_label",
                "description": "List of human interpreted category names",
                "kind": "STRING_SEQ",
                "optional": true,
                "unit": "n/a"
            },
            {
                "name": "directed",
                "description": "Determines if this graph is directed",
                "kind": "BOOLEAN",
                "optional": true,
                "unit": "n/a"
            },
            {
                "name": "layout",
                "description": "List of Graph Nodes representing this graph",
                "kind": "MULTI_DIM_NVP",
                "dimensions": 1,
                "unit": "n/a",
                "typedefinition": "GraphNode"
            }
        ]
    },
    {
        "typedefinition": "GraphNode",
        "tags": [
            {
                "name": "origin_vertex",
                "description": "A vertex which will serve as the origin",
                "kind": "INT64",
                "unit": "An index of a DetectionPointData"
            },
            {
                "name": "destination_vertex",
                "description": "A vertex which will serve as the destination",
                "kind": "INT64",
                "unit": "An index of a DetectionPointData"
            },
            {
                "name": "label",
                "description": "List of labels associated with this edge",
                "kind": "STRING_SEQ",
                "optional": true,
                "unit": "n/a"
            }
        ]
    }
]

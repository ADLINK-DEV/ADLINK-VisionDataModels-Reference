[
   {
      "name":"DataCodes",
      "context":"com.adlinktech.vision.inference",
      "qosProfile":"telemetry",
      "version":"2.2.0",
      "description":"Inference engine results for detected data codes. The stream_id is not part of this TagGroup and assumed to be part of the run-time flowId.",
      "tags":[
         {
            "name":"engine_id",
            "description":"Inference engine identifier",
            "kind":"STRING",
            "optional":true,
            "unit":"UUID"
         },
         {
            "name":"frame_id",
            "description":"ID of the input video frame fed to the inference engine",
            "kind":"INT64",
            "unit":"NUM"
         },
         {
            "name":"codes",
            "description":"List of DataMatrix codes (the results)",
            "kind":"MULTI_DIM_NVP",
            "dimensions":1,
            "unit":"n/a",
            "typedefinition":"DataCode"
         }
      ]
   },
   {
      "typedefinition":"DataCode",
      "tags":[
         {
            "name":"id",
            "description":"DataMatrix code ID",
            "kind":"STRING",
            "unit":"n/a"
         },
         {
            "name":"type",
            "description":"Data code type (ECC200, etc.)",
            "kind":"STRING",
            "unit":"n/a"
         },
         {
            "name":"x1",
            "description":"Top Left X Coordinate (% from 0,0) in pixels in src image",
            "kind":"FLOAT64",
            "unit":"Percentage"
         },
         {
            "name":"y1",
            "description":"Top Left Y Coordinate (% from 0,0) in pixels in src image",
            "kind":"FLOAT64",
            "unit":"Percentage"
         },
         {
            "name":"x2",
            "description":"Bottom Right X Coordinate (% from 0,0) in pixels in src image",
            "kind":"FLOAT64",
            "unit":"Percentage"
         },
         {
            "name":"y2",
            "description":"Bottom Right Y Coordinate (% from 0,0) in pixels in src image",
            "kind":"FLOAT64",
            "unit":"Percentage"
         },
         {
            "name":"phi",
            "description":"Rotation of data code",
            "kind":"FLOAT64",
            "optional":true,
            "unit":"radians"
         },
         {
            "name":"color",
            "description":"Detection box color",
            "kind":"STRING",
            "optional":true,
            "unit":"Hex Code #RRGGBB"
         },
         {
            "name":"meta",
            "description":"Buffer for data code metadata",
            "kind":"STRING",
            "optional":true,
            "unit":"N/A"
         },
         {
            "name":"physical_coords",
            "description":"Real world location of the data code",
            "kind":"NVP_SEQ",
            "optional":true,
            "unit":"n/a",
            "typedefinition":"PhysicalCoords"
         }
      ]
   },
   {
      "typedefinition":"PhysicalCoords",
      "tags":[
         {
            "name":"x",
            "description":"X coordinate distance from the center of the image",
            "kind":"FLOAT64",
            "unit":"meters"
         },
         {
            "name":"y",
            "description":"Y coordinate distance from the center of the image",
            "kind":"FLOAT64",
            "unit":"meters"
         },
         {
            "name":"z",
            "description":"Z coordinate distance from the configured floor",
            "kind":"FLOAT64",
            "unit":"meters"
         },
         {
            "name":"width",
            "description":"Physical size of the detection box width edge",
            "kind":"FLOAT64",
            "unit":"meters"
         },
         {
            "name":"length",
            "description":"Physical size of the detection box length edge",
            "kind":"FLOAT64",
            "unit":"meters"
         }
      ]
   }
]

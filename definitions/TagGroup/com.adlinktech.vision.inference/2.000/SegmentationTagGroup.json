[
    {
        "name":"Segmentation",
        "context":"com.adlinktech.vision",
        "qosProfile":"telemetry",
        "version":"2.1.0",
        "description":"Inference engine results for classification model. The stream_id is supposed to be encapsulated in the flow_id",
        "tags":[
            {
                "name":"engine_id",
                "description":"Inference engine identifier",
                "kind":"STRING",
                "unit":"UUID"
            },
            {
                "name":"stream_id",
                "description":"ID of the stream fed into the inference engine",
                "kind":"STRING",
                "unit":"UUID"
            },
            {
                "name":"frame_id",
                "description":"ID of the input video frame fed to the inference engine",
                "kind":"INT64",
                "unit":"NUM"
            },
            {
                "name":"model_id",
                "description":"Unique AI model identifier",
                "kind":"STRING",
                "unit":"n/a"
            },
            {
                "name":"width",
                "description":"Segmentation mask frame width",
                "kind":"INT64",
                "unit":"pixels"
            },
            {
                "name":"height",
                "description":"Segmentation mask frame height",
                "kind":"INT64",
                "unit":"pixels"
            },
            {
                "name":"data",
                "description":"List of Segmentation Data (the results)",
                "kind":"NVP_SEQ",
                "unit":"n/a",
                "typedefinition": "SegmentationData"
            }
        ]
    },
    {
        "typedefinition": "SegmentationData",
        "tags": [
            {
                "name":"tracker_obj_id",
                "description":"Detected object id",
                "kind":"INT64",
                "optional":true,
                "unit":"UUID"
            },
            {
                "name":"category_id",
                "description":"Mask cateogry/class identifer",
                "kind":"INT64",
                "unit":"n/a"
            },
            {
                "name":"category_label",
                "description":"Human interpreted category name of the mask",
                "kind":"STRING",
                "unit":"n/a"
            },
            {
                "name":"size",
                "description":"Segmentation mask payload size (bytes)",
                "kind":"INT64",
                "unit":"bytes"
            },
            {
                "name":"channel",
                "description":"Segmentation mask output channel",
                "kind":"INT64",
                "unit":"index"
            },
            {
                "name":"color",
                "description":"Segmentation mask color (#XXXXXX)",
                "kind":"STRING",
                "optional":true,
                "unit":"Color"
            },
            {
                "name":"probability",
                "description":"Network confidence",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"frame_data",
                "description":"Segmentation mask frame data",
                "kind":"BYTE_SEQ",
                "unit":"bytes"
            },
            {
                "name":"uuid",
                "description":"Unique inference identifier",
                "kind":"STRING",
                "optional":true,
                "unit":"UUID"
            },
            {
                "name":"parent_id",
                "description":"Identifier referencing a parent inference node or result",
                "kind":"STRING",
                "optional":true,
                "unit":"n/a"
            }
        ]
    }
]

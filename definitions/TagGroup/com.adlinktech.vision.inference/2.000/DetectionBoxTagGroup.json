[
    {
        "name":"DetectionBox",
        "context":"com.adlinktech.vision",
        "qosProfile":"telemetry",
        "version":"2.1.0",
        "description":"Inference engine results for object detection model outputing bounding boxes",
        "tags":[
            {
                "name":"engine_id",
                "description":"Inference engine identifier",
                "kind":"STRING",
                "unit":"UUID"
            },
            {
                "name":"stream_id",
                "description":"ID of the stream fed into the inference engine",
                "kind":"STRING",
                "unit":"UUID"
            },
            {
                "name":"frame_id",
                "description":"ID of the input video frame fed to the inference engine",
                "kind":"INT64",
                "unit":"NUM"
            },
            {
                "name":"model_id",
                "description":"Unique AI model identifier",
                "kind":"STRING",
                "unit":"n/a"
            },
            {
                "name":"data",
                "description":"List of Detection Box Data (the results)",
                "kind":"NVP_SEQ",
                "unit":"n/a",
                "typedefinition": "DetectionBoxData"
            }
        ]
    },
    {
        "typedefinition": "DetectionBoxData",
        "tags": [
            {
                "name":"tracker_obj_id",
                "description":"Detected object id",
                "kind":"INT64",
                "optional":true,
                "unit":"UUID"
            },
            {
                "name":"super_category_id",
                "description":"Super Category ID containing the category",
                "kind":"INT64",
                "unit":"n/a"
            },
            {
                "name":"super_category_label",
                "description":"Human interpreted super category name",
                "kind":"STRING",
                "unit":"n/a"
            },
            {
                "name":"category_id",
                "description":"Classification category/class ID",
                "kind":"INT64",
                "unit":"n/a"
            },
            {
                "name":"category_label",
                "description":"Human interpreted category name",
                "kind":"STRING",
                "unit":"n/a"
            },
            {
                "name":"x1",
                "description":"Top Left X Coordinate (% from 0,0)",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"y1",
                "description":"Top Left Y Coordinate (% from 0,0)",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"x2",
                "description":"Bottom Right X Coordinate (% from 0,0)",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"y2",
                "description":"Bottom Right Y Coordinate (% from 0,0)",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"probability",
                "description":"Network confidence",
                "kind":"FLOAT64",
                "unit":"Percentage"
            },
            {
                "name":"color",
                "description":"Detection box color",
                "kind":"STRING",
                "optional":true,
                "unit":"N/A"
            },
            {
                "name":"metadata",
                "description":"Buffer for extra inference metadata",
                "optional":true,
                "kind":"STRING",
                "unit":"N/A"
            },
            {
                "name":"uuid",
                "description":"Unique inference identifier",
                "kind":"STRING",
                "optional":true,
                "unit":"UUID"
            },
            {
                "name":"parent_id",
                "description":"Identifier referencing a parent inference node or result",
                "kind":"STRING",
                "optional":true,
                "unit":"n/a"
            },
            {
                "name":"physical_coords",
                "description":"Real world location of the bounding box",
                "kind":"NVP_SEQ",
                "unit":"n/a",
                "typedefinition": "PhysicalCoords"
            }
        ]
    },
    {
        "typedefinition": "PhysicalCoords",
        "tags": [
            {
                "name":"x",
                "description":"X coordinate distance from the center of the image",
                "kind":"FLOAT64",
                "unit":"meters"
            },
            {
                "name":"y",
                "description":"Y coordinate distance from the center of the image",
                "kind":"FLOAT64",
                "unit":"meters"
            },
            {
                "name":"z",
                "description":"Z coordinate distance from the configured floor",
                "kind":"FLOAT64",
                "unit":"meters"
            },
            {
                "name":"width",
                "description":"Physical size of the detection box width edge",
                "kind":"FLOAT64",
                "unit":"meters"
            },
            {
                "name":"length",
                "description":"Physical size of the detection box length edge",
                "kind":"FLOAT64",
                "unit":"meters"
            }
        ]
    }
]
